    763          /*********************************************************************
    764           * @fn      rxCB
    765           *
    766           * @brief   Process UART Rx event handling.
    767           *          May be triggered by an Rx timer expiration - less than max
    768           *          Rx bytes have arrived within the Rx max age time.
    769           *          May be set by failure to alloc max Rx byte-buffer for the DMA Rx -
    770           *          system resources are too low, so set flow control?
    771           *
    772           * @param   none
    773           *
    774           * @return  none
    775           */
    776          static void rxCB( uint8 port, uint8 event )
    777          {
    778            uint8 *buf, len;
    779          
    780            /* While awaiting retries/response, only buffer 1 next buffer: otaBuf2.
    781             * If allow the DMA Rx to continue to run, allocating Rx buffers, the heap
    782             * will become so depleted that an incoming OTA response cannot be received.
    783             * When the Rx data available is not read, the DMA Rx Machine automatically
    784             * sets flow control off - it is automatically re-enabled upon Rx data read.
    785             * When the back-logged otaBuf2 is sent OTA, an Rx data read is scheduled.
    786             */
    787            if ( otaBuf2 )
    788            {
    789              return;
    790            }
    791          
    792            if ( !(buf = osal_mem_alloc( SERIAL_APP_RX_CNT )) )
    793            {
    794              return;
    795            }
    796          
    797            /* HAL UART Manager will turn flow control back on if it can after read.
    798             * Reserve 1 byte for the 'sequence number'.
    799             */
    800            len = HalUARTRead( port, buf+1, SERIAL_APP_RX_CNT-1 );
    801          
    802            if ( !len )  // Length is not expected to ever be zero.
    803            {
    804              osal_mem_free( buf );
    805              return;
    806            }
    807          
    808            /* If the local global otaBuf is in use, then either the response handshake
    809             * is being awaited or retries are being attempted. When the wait/retries
    810             * process has been exhausted, the next OTA msg will be attempted from
    811             * otaBuf2, if it is not NULL.
    812             */
    813            if ( otaBuf )
    814            {
    815              otaBuf2 = buf;
    816              otaLen2 = len;
    817            }
    818            else
    819            {
    820              otaBuf = buf;
    821              otaLen = len;
    822              /* Don't call SerialApp_SendData() from here in the callback function.
    823               * Set the event so SerialApp_SendData() runs during this task's time slot.
    824               */
    825              osal_set_event( SerialApp_TaskID, SERIALAPP_MSG_SEND_EVT );
    826            }
    827          }
    828          #endif
