    770          /*********************************************************************
    771           * @fn      rxCB
    772           *
    773           * @brief   Process UART Rx event handling.
    774           *          May be triggered by an Rx timer expiration - less than max
    775           *          Rx bytes have arrived within the Rx max age time.
    776           *          May be set by failure to alloc max Rx byte-buffer for the DMA Rx -
    777           *          system resources are too low, so set flow control?
    778           *
    779           * @param   none
    780           *
    781           * @return  none
    782           */
    783          static void rxCB( uint8 port, uint8 event )
    784          {
    785            uint8 *buf, len;
    786          
    787            /* While awaiting retries/response, only buffer 1 next buffer: otaBuf2.
    788             * If allow the DMA Rx to continue to run, allocating Rx buffers, the heap
    789             * will become so depleted that an incoming OTA response cannot be received.
    790             * When the Rx data available is not read, the DMA Rx Machine automatically
    791             * sets flow control off - it is automatically re-enabled upon Rx data read.
    792             * When the back-logged otaBuf2 is sent OTA, an Rx data read is scheduled.
    793             */
    794            if ( otaBuf2 )
    795            {
    796              return;
    797            }
    798          
    799            if ( !(buf = osal_mem_alloc( SERIAL_APP_RX_CNT )) )
    800            {
    801              return;
    802            }
    803          
    804            /* HAL UART Manager will turn flow control back on if it can after read.
    805             * Reserve 1 byte for the 'sequence number'.
    806             */
    807            len = HalUARTRead( port, buf+1, SERIAL_APP_RX_CNT-1 );
    808          
    809            if ( !len )  // Length is not expected to ever be zero.
    810            {
    811              osal_mem_free( buf );
    812              return;
    813            }
    814          
    815            /* If the local global otaBuf is in use, then either the response handshake
    816             * is being awaited or retries are being attempted. When the wait/retries
    817             * process has been exhausted, the next OTA msg will be attempted from
    818             * otaBuf2, if it is not NULL.
    819             */
    820            if ( otaBuf )
    821            {
    822              otaBuf2 = buf;
    823              otaLen2 = len;
    824            }
    825            else
    826            {
    827              otaBuf = buf;
    828              otaLen = len;
    829              /* Don't call SerialApp_SendData() from here in the callback function.
    830               * Set the event so SerialApp_SendData() runs during this task's time slot.
    831               */
    832              osal_set_event( SerialApp_TaskID, SERIALAPP_MSG_SEND_EVT );
    833            }
    834          }
