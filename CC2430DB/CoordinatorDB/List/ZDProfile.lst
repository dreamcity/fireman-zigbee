   1322            }
   1323          
   1324            pBuf = buf+1;
   1325          
   1326            *pBuf++ = Status;
   1327            *pBuf++ = NeighborLqiEntries;
   1328            *pBuf++ = StartIndex;
   1329            *pBuf++ = NeighborLqiCount;
   1330          
   1331            for ( x = 0; x < NeighborLqiCount; x++ )
   1332            {
   1333              osal_cpyExtAddr( pBuf, list->extPanID);         // Extended PanID
   1334              pBuf += Z_EXTADDR_LEN;
