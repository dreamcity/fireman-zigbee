    820            {
    821              osal_mem_free( buf );
    822              return;
    823            }
    824          
    825            /* If the local global otaBuf is in use, then either the response handshake
    826             * is being awaited or retries are being attempted. When the wait/retries
    827             * process has been exhausted, the next OTA msg will be attempted from
    828             * otaBuf2, if it is not NULL.
    829             */
    830            if ( otaBuf )
    831            {
    832              otaBuf2 = buf;
    833              otaLen2 = len;
    834            }
    835            else
    836            {
    837              otaBuf = buf;
    838              otaLen = len;
