//  252 
//  253   /* wait for one backoff to guarantee receiver has been on at least that long */
//  254   RFST = WAITW(1);
        MOV     0xe1,#-0x7f
//  255   RFST = WEVENT;
        MOV     0xe1,#-0x48
//  256   
//  257   /* sample CCA, if it fails exit from here, CSPZ indicates result */
//  258   RFST = SKIP(1, C_CCA_IS_VALID);
