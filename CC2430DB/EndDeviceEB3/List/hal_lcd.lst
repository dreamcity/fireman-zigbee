    340          /******************************************************************************
    341          * See lcd.h for a description of this function.
    342          ******************************************************************************/
    343          static void lcdUpdateLine(uint8 line, uint8 *pLine) {
    344            LCD_write_english_string(0,line,pLine);
    345          }
