    265          /*-----------------------------------------------------------------------
    266          LCD_write_char    : 显示英文字符
    267          
    268          输入参数：c       ：显示的字符；
    269          
    270          编写日期          ：2004-8-10
    271          最后修改日期      ：2004-8-10
    272          -----------------------------------------------------------------------*/
    273          static void LCD_write_char(unsigned char c)
    274          {
    275            unsigned char line;
    276          
    277            c -= 32;
    278            for (line=0; line<6; line++)
    279              LCD_write_byte(font6x8[c][line], 1);
    280          }
